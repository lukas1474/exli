{
  "compilerOptions": {
    "noImplicitAny": true,
    "outDir": "build/types",
    "module": "esnext",
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["es2018", "dom"],
    "moduleResolution": "node",
    "strict": true
  },
  "include": ["./src/**/*", "tests/", "src/Myfunction.ts", "src/index.ts", "src/Countries.ts", "src/CompareCountries.ts", "src/EUCountries.ts", "src/Api.ts", "config.js", "config.js"],
  "exclude": ["tests/"],
}
